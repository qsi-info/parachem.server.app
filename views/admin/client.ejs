<div class="col-md-6">
	<h3>
		Applications
		<a href="/admin/client/new" class="add-link">
			<i class="fa fa-plus-circle"></i>&nbsp;new client application
		</a>		
	</h3>

	<div class="metro">
	<% clients.forEach(function (client) { %>
		<a href="/admin/client/dashboard/<%= client.id %>">
			<div class="tile bg-cyan">
			  <div class="tile-content icon">
			  	<i class="<%= client.icon %>"></i>
			  </div>
			  <div class="tile-status">
			    <span class="name"><%= client.name %></span>
			  </div>
			</div>
		</a>	
	<% }) %>
	</div>

</div>









