<h1>
  Internal Server Error
</h1>
 <% if (typeof errors !== 'undefined') { %>
 <pre class="error"><code>
<% _.each(errors, function (error) { %>
  <%= JSON.stringify(error) %>
  <% }); %>
</code></pre>
<% } else { %>

<p class="error">
  A team of highly trained sea bass is working on this as we speak.
</p> 
<% } %>











